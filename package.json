  "description": "webpack multiple entry and output",
  "keywords": [
    "webpack",
    "pug",
    "scss",
    "babel",
    "core-js"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.js --progress --open",
    "prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js --progress"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
