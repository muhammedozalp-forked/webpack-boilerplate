  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.js --progress",
    "prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js --progress"
  },
  "browserslist": {
    "production": [
